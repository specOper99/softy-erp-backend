      ],
      "request": {
        "headers": {
          "Authorization": "Bearer eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9..."
        },
        "matchingRules": {
          "header": {
            "Authorization": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            }
          }
        },
        "method": "GET",
        "path": "/bookings"
      },
      "response": {
        "body": {
          "data": [
            {
              "clientName": "Client A",
              "eventDate": "2026-01-15T10:00:00.000Z",
              "id": "uuid-booking-1",
              "status": "confirmed",
              "totalPrice": 1500
            }
          ],
          "limit": 10,
          "page": 1,
          "total": 1
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "body": {
            "$.data": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type",
                  "min": 1
                }
              ]
            },
            "$.data[*].clientName": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.data[*].eventDate": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.data[*].id": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.data[*].status": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "regex",
                  "regex": "confirmed|cancelled|completed|pending"
                }
              ]
            },
            "$.data[*].totalPrice": {
              "combine": "AND",
              "matchers": [
