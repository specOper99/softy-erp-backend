{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "transformIgnorePatterns": [
    "node_modules/(?!(uuid)/)"
  ],
  "moduleDirectories": [
    "node_modules",
    "<rootDir>/../"
  ],
  "setupFilesAfterEnv": [
    "<rootDir>/jest-setup.ts",
    "<rootDir>/jest-e2e-setup.ts"
  ]
}