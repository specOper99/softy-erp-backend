    "decimal.js": "^10.6.0",
    "dotenv": "^17.2.3",
    "file-type": "^21.3.0",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "ioredis": "^5.9.2",
    "json2csv": "^6.0.0-alpha.2",
    "lru-cache": "^11.2.4",
    "msgpackr": "^1.11.8",
    "nest-winston": "^1.10.2",
    "node-vault": "^0.10.9",
    "nodemailer": "^7.0.12",
    "opossum": "^9.0.0",
    "otplib": "^12.0.1",
    "p-limit": "^7.2.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pdf-lib": "^1.17.1",
    "pg": "^8.16.3",
    "prom-client": "^15.1.3",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sanitize-html": "^2.17.0",
    "socket.io": "^4.8.3",
    "stripe": "^20.1.0",
    "typeorm": "^0.3.28",
    "unleash-client": "^6.9.0",
    "uuid": "^11.1.0",
    "winston": "^3.19.0",
    "winston-loki": "^6.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.1.11",
    "@pact-foundation/pact": "^16.0.4",
    "@stryker-mutator/core": "^9.4.0",
    "@stryker-mutator/jest-runner": "^9.4.0",
