{
  "name": "softy-erp",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix --cache",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:cov:all": "jest --config ./test/jest-all.json --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:pact": "jest --config ./jest.pact.config.js",
    "test:integration": "jest --config ./jest.integration.config.js",
    "test:integration:cov": "jest --config ./jest.integration.config.js --coverage",
    "test:stability": "for i in {1..3}; do npm run test -- --runInBand; done",
    "test:load:auth": "k6 run scripts/load-testing/scenarios/auth-flow.js",
    "test:load:booking": "k6 run scripts/load-testing/scenarios/booking-flow.js",
    "test:load:finance": "k6 run scripts/load-testing/scenarios/finance-flow.js",
    "test:load:stress": "k6 run scripts/load-testing/scenarios/stress-test.js",
    "test:load:all": "npm run test:load:auth && npm run test:load:booking && npm run test:load:finance",
    "seed": "ts-node -r tsconfig-paths/register src/database/seed.ts",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d src/database/data-source.ts",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/database/data-source.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/database/data-source.ts",
    "migration:show": "typeorm-ts-node-commonjs migration:show -d src/database/data-source.ts",
    "backup": "bash scripts/backup.sh",
    "load-test": "k6 run load-tests/api-load-test.js",
    "db:reset:test": "ts-node -r tsconfig-paths/register scripts/reset-test-db.ts",
    "platform:create-admin": "ts-node -r tsconfig-paths/register scripts/platform/create-admin-auto.ts",
    "prepare": "husky",
    "type-check": "tsc --noEmit -p tsconfig.typecheck.json",
    "lint:circular": "madge --circular src/",
    "lint:duplication": "jscpd src --ignore \"**/*.spec.ts,**/*.test.ts,**/node_modules/**,**/dist/**,**/*.d.ts\" --reporters console,json",
    "check:sql-safety": "ts-node scripts/ci/check-raw-queries.ts",
    "validate": "npm run lint && npm run type-check && npm run test",
    "pre-push": "npm run validate",
    "test:mutation": "stryker run"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.958.0",
    "@aws-sdk/s3-request-presigner": "^3.958.0",
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/cache-manager": "^3.1.0",
