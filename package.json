    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/memcached": "^2.2.10",
    "@types/multer": "^2.0.0",
    "@types/mysql": "^2.15.27",
    "@types/node": "^25.0.3",
    "@types/node-vault": "^0.9.13",
    "@types/nodemailer": "^7.0.5",
    "@types/opossum": "^8.1.9",
    "@types/passport-jwt": "^4.0.1",
    "@types/qrcode": "^1.5.6",
    "@types/sanitize-html": "^2.16.0",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^11.0.0",
    "buffer-from": "^1.1.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^30.0.0",
    "jest-junit": "^16.0.0",
    "jscpd": "^4.0.7",
    "lint-staged": "^16.2.7",
    "madge": "^8.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "sqlite3": "^5.1.7",
    "supertest": "^7.0.0",
    "testcontainers": "^10.28.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.51.0"
  },
  "overrides": {
    "nodemailer": "^7.0.12",
    "diff": "^8.0.3",
    "glob": "^10.5.0",
    "mjml": "file:./scripts/dummy-mjml",
    "qs": "^6.14.1",
    "tar": "^7.5.3",
    "undici": "^6.23.0",
    "@nestjs/typeorm": "^11.0.0",
    "uuid": "^11.1.0"
  },
  "jest": {
    "maxWorkers": 2,
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(uuid|p-limit|yocto-queue)/)"
    ],
    "coverageProvider": "v8",
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "!main.ts",
      "!instrument.ts",
      "!**/*.module.ts",
      "!**/*.dto.ts",
      "!**/*.entity.ts",
      "!**/index.ts",
      "!**/*.config.ts",
      "!common/telemetry/**",
      "!database/migrations/**",
      "!database/seeds/**",
      "!database/data-source.ts",
      "!database/seed.ts"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/../test/jest-setup.ts"
    ],
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "reports",
          "outputName": "jest-junit.xml"
        }
      ]
    ]
  }
}
