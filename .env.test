# Test Environment Overrides
# This file is loaded after .env during E2E tests to override specific values.

# Disable global rate limiting to prevent 429 errors during fast test execution
DISABLE_RATE_LIMITING=true

# Ensure Redis is disabled or mocked (setup file mocks the valid module, but we explicit here)
REDIS_URL=

# E2E auth/seeding (TEST-ONLY values; not for production)
JWT_SECRET=e2e-only-jwt-secret-2026-01-18
SEED_ADMIN_PASSWORD=e2e-only-admin-password-2026-01-18
SEED_STAFF_PASSWORD=e2e-only-staff-password-2026-01-18
SEED_OPS_PASSWORD=e2e-only-ops-password-2026-01-18

# You can add other test-specific overrides here
DB_DATABASE=chapters_studio_test

# Keep schema synchronization off (production-like)
DB_SYNCHRONIZE=false

# E2E safety: disable schema reset to avoid index conflicts
E2E_DB_RESET=false
